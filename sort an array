//Html
<!DOCTYPE html>
<html lang="en">
<head>
    
    <title>sorting</title>
</head>
<body>
    <script>
        var arr=[9,2,8,4,7,3];
        let length=arr.length;
        for(let i=0; i<length-1;i++){
            if(arr [i]>arr[i+1]){
                let temp=arr[i];
                arr[i] = arr[i+1];
                arr[i+1] = temp;
                i = -1;
            }
        }
        
        console.log(arr);
    </script>
    
</body>
</html>